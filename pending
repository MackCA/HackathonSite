                           })

                       }
                       break;
                   }
               case 6:
                   {
                       function writeUserData(userId, teamName, memberName1, memberName2, memberName3, projectSummary, emblem) {
                           firebase.database().ref('users/' + userId).set({
                               teamName: teamData[0],
